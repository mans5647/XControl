    "version": "0.2.0",
    "configurations": [
        {
            "name": "launch golang",
            "type": "go",
            "request": "launch",
            "mode": "debug",
            
            "program": "${workspaceFolder}/go_server/cmd/main.go"
        },
        {
            "name": "(gdb) Attach",
            "type": "cppdbg",
            "request": "attach",
            "program": "${workspaceFolder}/build/myabc_out/bin/myabc.exe",
            "MIMode": "lldb",
            "miDebuggerPath": "C:/Program Files/LLVM/bin/lldb.exe",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                },
                {
                    "description": "Set Disassembly Flavor to Intel",
                    "text": "-gdb-set disassembly-flavor intel",
                    "ignoreFailures": true
                }
            ]
        },
        {
            "name": "main app",
            "type": "cppvsdbg",
            "request": "launch",
            "program": "${workspaceFolder}/build/XControlAgent.exe",
            "args": [],
            "stopAtEntry": false,
            "cwd": "${fileDirname}",
            "environment": [],
            "console": "integratedTerminal",
            
        },
        {
            "name": "test app (file utils)",
            "type": "cppvsdbg",
            "request": "launch",
            "program": "${workspaceFolder}/build/myabc_out/bin/test_io.exe",
